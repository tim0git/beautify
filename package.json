{
  "name": "beautify",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios12Mini": "react-native run-ios --simulator='iPhone 12 mini'",
    "start": "react-native start",
    "test": "jest",
    "test-openHandles": "jest --detectOpenHandles",
    "detox-test-r": "detox test --reuse",
    "lint": "eslint ./src",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "prestorybook": "rnstl",
    "cleanbuild": "./scripts/cleanBuild.sh",
    "fullinstall": "npm install && cd ios && pod install && cd ..",
    "postinstall": "patch-package",
    "build:ios-debug": "detox build -c ios.sim.debug",
    "build:ios-release": "detox build -c ios.sim.release",
    "build:android-debug": "detox build -c android.emu.debug",
    "build:android-release": "detox build -c android.emu.release",
    "test:ios-debug": "detox test -c ios.sim.debug -l verbose",
    "test:ios-release": "detox test -c ios.sim.release -l verbose",
    "test:android-debug": "detox test -c android.emu.debug -l verbose",
    "test:android-release": "detox test -c android.emu.release -l verbose",
    "e2e:ios-debug": "npm run build:ios-debug && npm run test:ios-debug",
    "e2e:ios-release": "npm run build:ios-release && npm run test:ios-release",
    "e2e:android-debug": "npm run build:android-debug && npm run test:android-debug",
    "e2e:android-release": "npm run build:android-release && npm run test:android-release",
    "list-ios-devices": "xcrun simctl list devices"
  },
  "dependencies": {
    "@apollo/client": "^3.3.8",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/app": "^10.6.4",
    "@react-native-firebase/auth": "^10.6.4",
    "@react-navigation/bottom-tabs": "^5.11.7",
    "@react-navigation/native": "^5.9.2",
    "@react-navigation/stack": "^5.14.2",
    "@twilio/conversations": "^1.1.0",
    "graphql": "^15.5.0",
    "moment": "^2.29.1",
    "react": "16.13.1",
    "react-moment": "^1.1.1",
    "react-native": "0.63.4",
    "react-native-calendars": "^1.1249.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-get-random-values": "^1.5.1",
    "react-native-maps": "0.27.1",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.17.1",
    "react-native-vector-icons": "^8.0.0",
    "react-native-webview": "^11.2.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "redux-saga-firebase": "^0.15.0",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.23",
    "@storybook/react-native": "^5.3.23",
    "@storybook/react-native-server": "^5.3.23",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.1",
    "@types/react-native": "^0.63.48",
    "@types/react-redux": "^7.1.16",
    "@types/uuid": "^8.3.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "detox": "^18.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.19.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "metro-react-native-babel-preset": "^0.65.0",
    "mockdate": "^3.0.2",
    "patch-package": "^6.2.2",
    "react-dom": "^16.13.1",
    "react-native-storybook-loader": "^2.0.2",
    "react-test-renderer": "16.13.1",
    "redux-saga-test-plan": "^4.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint --fix",
      "pre-push": "npm test && npm run lint --fix"
    }
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "./src/setupTests.ts"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src",
        "./packages"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
